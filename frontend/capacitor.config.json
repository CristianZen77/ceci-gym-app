{
  "appId": "com.fitwoman.app",
  "appName": "Fit Woman",
  "webDir": "build",
  "server": {
    "androidScheme": "https",
    "cleartext": true
  },
  "android": {
    "backgroundColor": "#ff69b4",
    "icon": "./public/logo.png",
    "buildOptions": {
      "keystorePath": "release-key.keystore",
      "keystoreAlias": "key0",
      "minSdkVersion": 21,
      "targetSdkVersion": 33
    }
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 3000,
      "backgroundColor": "#ff69b4",
      "androidScaleType": "CENTER_CROP",
      "splashFullScreen": true,
      "splashImmersive": true,
      "splashImage": "./public/logo.png"
    }
  },
  "app": {
    "appName": "Fit Woman",
    "appVersion": "1.0.0",
    "appBuild": "1",
    "bundleId": "com.fitwoman.app",
    "description": "Aplicaci√≥n para reserva de clases de gimnasio",
    "author": {
      "name": "FitWoman Team",
      "email": "contact@fitwoman.com"
    }
  }
}
