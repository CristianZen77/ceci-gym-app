{
  "expo": {
    "name": "Fit Woman",
    "slug": "fit-woman",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/logo.png",
    "splash": {
      "image": "./public/logo.png",
      "resizeMode": "contain", 
      "backgroundColor": "#FF4081"  
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fitwoman.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/logo.png",
        "backgroundColor": "#FF4081"
      },
      "icon": "./public/logo.png",
      "package": "com.fitwoman.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./public/logo.png"
    }
  }
}
